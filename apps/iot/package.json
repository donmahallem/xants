{
  "name": "@donmahallem/xants-app-iot",
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "keywords": [
    "api",
    "types"
  ],
  "scripts": {
    "build": "tsc",
    "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json --silent true",
    "test": "mocha --config ../../.mocharc.yml",
    "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
    "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json src/**/*.ts",
    "docs": "typedoc --options ../../typedoc.json",
    "prepublishOnly": "npm run test && npm run docs && npm run build"
  },
  "homepage": "https://donmahallem.github.io/xants/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/donmahallem/xants/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/donmahallem/xants.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@manniwatch/api-proxy-router": "^0.18.0",
    "@manniwatch/api-types": "^0.18.0",
    "express": "~4.17.1",
    "helmet": "~4.3.1",
    "nconf": "~0.11.1"
  },
  "devDependencies": {
    "@types/express": "~4.17.9",
    "@types/helmet": "~4.0.0",
    "apidoc": "~0.26.0",
    "supertest": "~6.0.1",
    "tslint": "~6.1.3"
  }
}
